<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <description>Signup New User</description>
        <from-view-id>/public/signup.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/public/signupSuccess.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>failure</from-outcome>
            <to-view-id>/public/signupFailure.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>debug</from-outcome>
            <to-view-id>/public/signupDebug.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <description>Reactivate User</description>
        <from-view-id>/private/reactivate.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>reactivateSuccess</from-outcome>
            <to-view-id>/private/ReactivateSuccess.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>reactivateFailure</from-outcome>
            <to-view-id>/private/ReactivateFailure.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>deleteSuccess</from-outcome>
            <to-view-id>/private/DeleteSuccess.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>deleteFailure</from-outcome>
            <to-view-id>/private/DeleteFailure.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>logout</from-outcome>
            <to-view-id>/public/Logout.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <description>Reactivate Success</description>
        <from-view-id>/private/ReactivateSuccess.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>logout</from-outcome>
            <to-view-id>/public/Logout.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <description>Delete Success</description>
        <from-view-id>/private/DeleteSuccess.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>logout</from-outcome>
            <to-view-id>/public/Logout.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <description>Home Page</description>
        <from-view-id>/user/home.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>logout</from-outcome>
            <to-view-id>/public/Logout.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>viewSuccess</from-outcome>
            <to-view-id>/user/viewPlan.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>viewFailure</from-outcome>
            <to-view-id>/user/viewPlanFailure.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>deleteSuccess</from-outcome>
            <to-view-id>/user/deletePlanSuccess.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>deleteFailure</from-outcome>
            <to-view-id>/user/deletePlanFailure.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <description>User Profile</description>
        <from-view-id>/user/profile.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>updateSuccess</from-outcome>
            <to-view-id>/user/userUpdateSuccess.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>updateFailure</from-outcome>
            <to-view-id>/user/userUpdateFailure.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>deactivateSuccess</from-outcome>
            <to-view-id>/user/userDeactivateSuccess.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>deactivateFailure</from-outcome>
            <to-view-id>/user/userDeactivateFailure.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <description>Update User Password</description>
        <from-view-id>/user/updatePassword.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/user/passwordUpdateSuccess.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>failure</from-outcome>
            <to-view-id>/user/passwordUpdateFailure.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <description>User Deactivate Success</description>
        <from-view-id>/user/userDeactivateSuccess.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>logout</from-outcome>
            <to-view-id>/public/Logout.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <description>User Update Success</description>
        <from-view-id>/user/userUpdateSuccess.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>logout</from-outcome>
            <to-view-id>/public/Logout.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <description>Password Update Success</description>
        <from-view-id>/user/passwordUpdateSuccess.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>logout</from-outcome>
            <to-view-id>/public/Logout.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <description>Plans Page</description>
        <from-view-id>/user/plans.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>search</from-outcome>
            <to-view-id>/user/plans.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>logout</from-outcome>
            <to-view-id>/public/Logout.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>viewSuccess</from-outcome>
            <to-view-id>/user/viewPlan.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>viewFailure</from-outcome>
            <to-view-id>/user/viewPlanFailure.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>deleteSuccess</from-outcome>
            <to-view-id>/user/deletePlanSuccess.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>deleteFailure</from-outcome>
            <to-view-id>/user/deletePlanFailure.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <description>New Plan</description>
        <from-view-id>/user/newPlan.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/user/newPlanSuccess.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>failure</from-outcome>
            <to-view-id>/user/newPlanFailure.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <description>View Plan</description>
        <from-view-id>/user/viewPlan.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/user/editPlanSuccess.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>failure</from-outcome>
            <to-view-id>/user/editPlanFailure.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
